{
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 12
    // "sourceType": "module"
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "semi": true
      }
    ]
    // "eslint-plugin-eslint-plugin/naming-convention": [
    //   "error",
    //   {
    //     "selector": "variable",
    //     "format": ["camelCase"]
    //   },
    //   {
    //     "selector": "function",
    //     "format": ["camelCase"]
    //   },
    //   {
    //     "selector": "class",
    //     "format": ["PascalCase"]
    //   },
    //   {
    //     "selector": "enum",
    //     "format": ["PascalCase"]
    //   },
    //   {
    //     "selector": "enumMember",
    //     "format": ["UPPER_CASE"]
    //   }
    // ]
  },
  "plugins": ["prettier", "eslint-plugin-eslint-plugin"]
}
